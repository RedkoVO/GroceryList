{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react",
    "paths": {
      "@assets": ["./src/assets"],
      "@assets/*": ["./src/assets/*"],
      "@components": ["./src/components"],
      "@components/*": ["./src/components/*"],
      "@constants": ["./src/constants"],
      "@constants/*": ["./src/constants/*"],
      "@hooks": ["./src/hooks"],
      "@hooks/*": ["./src/hooks/*"],
      "@models": ["./src/models"],
      "@models/*": ["./src/models/*"],
      "@providers": ["./src/providers"],
      "@providers/*": ["./src/providers/*"],
      "@screens": ["./src/screens"],
      "@screens/*": ["./src/screens/*"],
      "@services": ["./src/services"],
      "@services/*": ["./src/services/*"],
      "@store": ["./src/store"],
      "@store/*": ["./src/store/*"],
      "@utils": ["./src/utils"],
      "@utils/*": ["./src/utils/*"],
    }
  }
}
